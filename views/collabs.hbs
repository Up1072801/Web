		<main>

		<div class="container my-5">
		  <h2>Collaborations</h2>

 		 </form>
		        {{#if isLoggedIn}}
                	<form action="/createCollab" method="POST">
                    <input type="text" name="Name" placeholder="Name" required>
                    <button type="submit">Submit</button>
                    </form>
					<form action="/CollabUpload" method="POST" enctype="multipart/form-data">
  					<input type="file" name="image" accept="image/jpeg">
					</form>
					<form action="/deleteCollab" method="POST">
					<input type="text" name="Name" placeholder="Title" required>
					<button type="submit">Delete</button>
					</form>
				{{/if}}

<div class="container-xxl">
			<div class="row">
			{{#each collabs}}
			<div class="col-5 my-3">	
			<div class="card">
				<img src="./img/{{{Name}}}.jpg" style="width: 515px; height: 300px;"  class="card-img-top" alt="...">
				<p class="card-text">{{{Name}}}</p>	
				
			</div>
			</div>
				{{/each}}
			  </div>
			
		  </div>
		</div>


		<!DOCTYPE html>

		</main>